<com.sssemil.advancedsettings.util.preference.PreferenceScreen
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.sssemil.advancedsettings.util.preference.ListPreference
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:pref_defaultValue="null"
        app:pref_entries="@array/entries_screen_timeout"
        app:pref_entryValues="@array/values_screen_timeout"
        app:pref_key="on_theatre_mode_launch_app"
        app:pref_title="@string/on_theatre_mode_launch_app" />

</com.sssemil.advancedsettings.util.preference.PreferenceScreen>
